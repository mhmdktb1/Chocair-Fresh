/* =========================================================
   RESET + VARIABLES
   Cleaned & Unified by ChatGPT
========================================================= */

:root {
  /* ===== Brand Colors ===== */
  --green: #2e7d32;
  --green-dark: #1b5e20;
  --green-light: #66bb6a;

  /* ===== Theme & Base Colors ===== */
  --bg: #ffffff;
  --text: #333333;
  --muted: #6f6f6f;
  --surface-soft: #f3fff1;

  /* ===== RGB for Shadows & Transitions ===== */
  --white-rgb: 255, 255, 255;
  --black-rgb: 0, 0, 0;
  --green-rgb: 46, 125, 50;
  --green-dark-rgb: 27, 94, 32;

  /* ===== Radius & Shadows ===== */
  --radius-sm: 10px;
  --radius-md: 18px;
  --radius-lg: 28px;

  --shadow-soft: 0 12px 32px rgba(var(--green-dark-rgb), 0.12);
  --shadow-card: 0 10px 24px rgba(var(--green-dark-rgb), 0.08);

  /* ===== Timing & Transitions ===== */
  --transition: all 0.3s ease;
}

/* =========================================================
   GLOBAL RESET
========================================================= */

*,
*::before,
*::after {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}

html {
  scroll-behavior: smooth;
  width: 100%;
  height: 100%;
  overflow-x: hidden;
}

body {
  min-height: 100vh;
  background: var(--bg);
  color: var(--text);
  font-family: "Cairo", system-ui, -apple-system, "Segoe UI", Roboto, Arial, sans-serif;
  overflow-x: hidden;
  transition: background 0.3s ease;
}

/* Disable scroll when mobile menu is open */
body.menu-open { overflow: hidden; }

/* =========================================================
   ELEMENTS BASE STYLES
========================================================= */

img {
  display: block;
  width: 100%;
  height: auto;
}

a {
  text-decoration: none;
  color: inherit;
  transition: color 0.2s ease;
}

button {
  font-family: inherit;
  border: none;
  background: none;
  color: inherit;
  cursor: pointer;
  padding: 0;
}

main {
  /* ensures content never hides under the header */
  padding-top: calc(100px + env(safe-area-inset-top, 0px));
}

/* =========================================================
   HEADER
   Cleaned & Optimized
========================================================= */

.header {
  position: fixed;
  top: 0; left: 0;
  width: 100%;
  background: transparent;
  transition: var(--transition);
  z-index: 100;
}

/* Container layout */
.header-container {
  max-width: 1200px;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 14px 18px;
}

/* =========================================================
   LOGO
========================================================= */
.logo {
  font-size: 1.6rem;
  font-weight: 800;
  color: #fff;
  letter-spacing: 0.3px;
  transition: color 0.3s ease;
}
.logo span { color: var(--green); }

/* When header scrolled */
.header.scrolled .logo { color: var(--green-dark); }
.header.scrolled .logo span { color: var(--green); }

/* =========================================================
   NAV LINKS
========================================================= */
.nav-links {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 32px;
  margin: 0 auto;
}

.nav-links a {
  position: relative;
  font-weight: 600;
  color: #fff;
  font-size: 1.05rem;
  padding: 4px 0;
  opacity: 0;
  transform: translateY(10px);
  animation: navFadeUp 0.6s ease forwards;
  transition: color 0.25s ease, transform 0.25s ease;
}

/* Fade-in order */
.nav-links a:nth-child(1) { animation-delay: 0.1s; }
.nav-links a:nth-child(2) { animation-delay: 0.2s; }
.nav-links a:nth-child(3) { animation-delay: 0.3s; }
.nav-links a:nth-child(4) { animation-delay: 0.4s; }

/* Hover underline */
.nav-links a::after {
  content: "";
  position: absolute;
  bottom: -4px;
  left: 0;
  width: 0%;
  height: 2px;
  background: var(--green);
  border-radius: 4px;
  transition: width 0.3s ease;
}
.nav-links a:hover::after { width: 100%; }

.nav-links a:hover {
  color: var(--green);
  transform: translateY(-2px);
}

@keyframes navFadeUp {
  from { opacity: 0; transform: translateY(10px); }
  to   { opacity: 1; transform: translateY(0); }
}

/* When scrolled */
.header.scrolled .nav-links a { color: var(--green-dark); }
.header.scrolled .nav-links a::after { background: var(--green-dark); }
.header.scrolled .nav-links a:hover { color: var(--green); }

/* =========================================================
   ICON BUTTONS
========================================================= */
.icon-btn {
  appearance: none;
  border: none;
  background: transparent;
  padding: 10px;
  border-radius: 16px;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  transition: all 0.25s ease;
  position: relative;
}

/* Lucide icons */
.icon-btn i,
.icon-btn svg {
  width: 24px;
  height: 24px;
  display: inline-block;
}

/* Default (header transparent): white stroke */
.icon-btn svg {
  stroke: #fff;
  stroke-width: 2;
  fill: none;
  transition: stroke 0.3s ease, transform 0.25s ease, filter 0.25s ease;
}

/* Hover effect (modern feel) */
.icon-btn:hover {
  background: rgba(255,255,255,0.15);
  box-shadow: 0 4px 10px rgba(0,0,0,0.15);
  transform: translateY(-2px) scale(1.05);
}
.icon-btn:active { transform: scale(0.95); }

/* Green color wave animation */
@keyframes iconGreenWave {
  0%   { stroke: #ffffff; }
  50%  { stroke: #66bb6a; }
  100% { stroke: #2e7d32; }
}
.icon-btn:hover svg {
  animation: iconGreenWave 0.7s ease forwards;
}

/* Icons appearance animation (enter) */
@keyframes iconFadeIn {
  from { opacity: 0; transform: translateY(8px) scale(0.9); }
  to   { opacity: 1; transform: translateY(0) scale(1); }
}
.actions .icon-btn:nth-child(1) { animation: iconFadeIn 0.5s ease forwards 0.1s; }
.actions .icon-btn:nth-child(2) { animation: iconFadeIn 0.5s ease forwards 0.25s; }
.actions .icon-btn:nth-child(3) { animation: iconFadeIn 0.5s ease forwards 0.4s; }

/* =========================================================
   SCROLLED HEADER STATE
========================================================= */
.header.scrolled {
  background: rgba(255,255,255,0.95);
  backdrop-filter: blur(10px);
  box-shadow: 0 8px 24px rgba(0,0,0,0.08);
  border-radius: 20px;
  width: 95%;
  left: 50%;
  transform: translateX(-50%);
}

.header.scrolled .icon-btn {
  background: rgba(46,125,50,0.08);
  box-shadow: none;
}
.header.scrolled .icon-btn:hover {
  background: rgba(46,125,50,0.12);
  box-shadow: 0 3px 8px rgba(46,125,50,0.25);
}

/* In scrolled state, icon color becomes green-dark */
.header.scrolled .icon-btn svg {
  stroke: var(--green-dark);
}
.header.scrolled .icon-btn:hover svg {
  animation: iconGreenWave 0.7s ease forwards;
}

/* =========================================================
   ICONS WRAPPER
========================================================= */
.actions {
  display: flex;
  flex-direction: row;
  align-items: center;
  gap: 12px;
}

/* Login button styling */
.login-btn {
  background: var(--green);
  color: #fff;
  padding: 8px 20px;
  border-radius: 999px;
  font-weight: 600;
  font-size: 0.95rem;
  transition: all 0.3s ease;
  white-space: nowrap;
}
.login-btn:hover {
  background: var(--green-dark);
  transform: translateY(-2px);
}


/* =========================================================
   HERO SECTION
   Cleaned & Optimized by ChatGPT
========================================================= */

.hero-section {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 80vh;
  overflow: hidden;
  text-align: center;
}

/* Background image */
.hero-image {
  position: absolute;
  inset: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center 40%;
  filter: brightness(70%);
  transition: transform 8s ease-out;
}

/* Smooth parallax effect on hover (optional subtle motion) */
.hero-section:hover .hero-image {
  transform: scale(1.02);
}

/* Content container */
.hero-content {
  position: relative;
  z-index: 2;
  color: #fff;
  padding: 0 16px;
  max-width: 90%;
}

/* Title */
.hero-content h1 {
  font-size: clamp(1.8rem, 4vw, 2.6rem);
  font-weight: 700;
  margin-bottom: 10px;
  letter-spacing: 0.4px;
  animation: fadeUp 0.8s ease forwards;
}

/* Subtitle */
.hero-content p {
  font-size: clamp(1rem, 2.5vw, 1.2rem);
  font-weight: 400;
  color: rgba(255,255,255,0.9);
  margin-bottom: 24px;
  animation: fadeUp 0.8s ease forwards 0.2s;
}

/* Button */
.hero-btn {
  display: inline-block;
  padding: 12px 30px;
  background: var(--green);
  color: #fff;
  font-weight: 600;
  border-radius: 999px;
  text-decoration: none;
  box-shadow: 0 8px 20px rgba(var(--green-rgb), 0.3);
  transition: background 0.3s ease, transform 0.25s ease, box-shadow 0.3s ease;
  animation: fadeUp 0.8s ease forwards 0.4s;
}

/* Hover effect */
.hero-btn:hover {
  background: var(--green-dark);
  transform: translateY(-3px);
  box-shadow: 0 10px 24px rgba(var(--green-dark-rgb), 0.35);
}

/* Subtle click feedback */
.hero-btn:active {
  transform: translateY(0);
  box-shadow: 0 4px 12px rgba(var(--green-dark-rgb), 0.2);
}

/* Fade-up animation */
@keyframes fadeUp {
  0% { opacity: 0; transform: translateY(20px); }
  100% { opacity: 1; transform: translateY(0); }
}


/* ================= RESPONSIVE ================= */
@media (min-width: 1201px) {
  .nav-links { display: flex !important; }
  .menu-btn { display: none !important; }
  .logo {
    position: relative;
    left: auto;
    transform: none;
  }
}

/* Tablet/zoomed desktop breakpoint - switch to mobile menu earlier */
@media (max-width: 1200px) {
  .nav-links { 
    display: none !important; 
  }
  .menu-btn { 
    display: inline-flex !important; 
  }
  
  /* Hide action icons on smaller screens */
  .actions a[aria-label="Search"], 
  .actions a[aria-label="Cart"],
  .actions a[aria-label="Account"] {
    display: none !important;
  }
  
  /* Center logo */
  .logo {
    position: absolute;
    left: 50%; 
    transform: translateX(-50%);
    font-size: 1.4rem;
  }
  
  /* Keep login button visible but smaller */
  .actions .login-btn {
    padding: 6px 14px;
    font-size: 0.85rem;
  }
  
  /* Ensure proper spacing */
  .header-container {
    padding: 12px 16px;
  }
  
  .actions {
    margin-left: auto;
  }
}

@media (max-width: 991px) {
  .nav-links { display: none; }
  .menu-btn { display: inline-flex; }
  
  /* Hide action icons on mobile (keep only hamburger and logo) */
  .actions a[aria-label="Search"], 
  .actions a[aria-label="Cart"],
  .actions a[aria-label="Account"] {
    display: none !important;
  }
  
  /* Keep login button visible but make it smaller */
  .actions .login-btn {
    padding: 6px 14px;
    font-size: 0.85rem;
  }
  
  /* Center logo on mobile */
  .logo {
    position: absolute;
    left: 50%; 
    transform: translateX(-50%);
    font-size: 1.4rem;
  }
  
  /* Ensure header container doesn't break */
  .header-container {
    padding: 12px 16px;
  }
  
  /* Make sure actions stay on right */
  .actions {
    margin-left: auto;
  }
}
/* =========================================================
   ğŸŒ¿ FEATURED CATEGORIES â€” PREMIUM MODERN DESIGN
========================================================= */
.featured-categories {
  padding: 90px 20px;
  background: linear-gradient(180deg, #f9fdf8 0%, #ffffff 100%);
  color: var(--green-dark);
}

/* ===== Section Header ===== */
.section-header {
  text-align: center;
  margin-bottom: 60px;
  animation: fadeIn 1s ease;
}
.section-header h2 {
  font-size: clamp(1.8rem, 4vw, 2.6rem);
  font-weight: 900;
  color: var(--green-dark);
  letter-spacing: 0.5px;
}
.section-header p {
  color: var(--muted);
  font-size: 1.05rem;
  margin-top: 8px;
}

/* ===== Grid Layout ===== */
.categories-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(340px, 1fr));
  gap: 40px;
  margin-bottom: 60px;
}

/* ===== Category Block ===== */
.category-block {
  background: #ffffff;
  border-radius: 24px;
  padding: 26px 24px 30px;
  box-shadow: 0 10px 24px rgba(0, 0, 0, 0.06);
  transition: transform 0.35s ease, box-shadow 0.35s ease;
  position: relative;
  overflow: hidden;
}
.category-block:hover {
  transform: translateY(-6px);
  box-shadow: 0 16px 32px rgba(0, 0, 0, 0.08);
}

/* Accent gradient highlight */
.category-block::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 6px;
  border-radius: 24px 24px 0 0;
  background: linear-gradient(90deg, var(--green), var(--green-dark));
}

/* ===== Category Header ===== */
.category-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 16px;
}
.category-header h3 {
  font-size: 1.4rem;
  font-weight: 800;
  color: var(--green-dark);
  display: flex;
  align-items: center;
  gap: 6px;
}
/* ===== Category Top (Title + Shop All) ===== */
.category-top {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
}

.shop-all {
  font-size: 0.95rem;
  font-weight: 700;
  color: var(--green);
  text-decoration: none;
  transition: color 0.25s ease, transform 0.25s ease;
  display: inline-flex;
  align-items: center;
  gap: 4px;
}
.shop-all::after {
  content: "â†’";
  font-size: 1rem;
  transition: transform 0.25s ease;
}
.shop-all:hover {
  color: var(--green-dark);
  transform: translateX(2px);
}
.shop-all:hover::after {
  transform: translateX(4px);
}


/* ===== Slider Buttons ===== */
.nav-buttons {
  display: flex;
  gap: 8px;
}
.slider-btn {
  width: 36px;
  height: 36px;
  border-radius: 50%;
  border: none;
  background: var(--green);
  color: #fff;
  font-size: 1.2rem;
  cursor: pointer;
  transition: background 0.25s ease, transform 0.25s ease;
}
.slider-btn:hover {
  background: var(--green-dark);
  transform: scale(1.1);
}

/* ===== Products Row ===== */
.products-row {
  display: flex;
  gap: 18px;
  overflow-x: auto;
  scroll-behavior: smooth;
  -webkit-overflow-scrolling: touch;
  scroll-snap-type: x mandatory;
  padding-bottom: 10px;
}
.products-row::-webkit-scrollbar { display: none; }

/* ===== Product Card ===== */
.product-card {
  flex: 0 0 150px;
  border-radius: 16px;
  overflow: hidden;
  background: #ffffff;
  box-shadow: 0 6px 14px rgba(0, 0, 0, 0.05);
  text-align: center;
  scroll-snap-align: start;
  transition: transform 0.3s ease, box-shadow 0.3s ease;
  animation: slideUp 0.8s ease both;
}
.product-card img {
  width: 100%;
  height: 130px;
  object-fit: cover;
  border-bottom: 2px solid rgba(46, 125, 50, 0.1);
}
.product-card figcaption {
  padding: 10px 6px;
  font-weight: 600;
  color: var(--green-dark);
  font-size: 0.95rem;
  background: #fff;
}
.product-card:hover {
  transform: translateY(-5px);
  box-shadow: 0 10px 20px rgba(46, 125, 50, 0.15);
}

/* ===== ANIMATIONS ===== */
@keyframes fadeIn {
  from { opacity: 0; transform: translateY(10px); }
  to { opacity: 1; transform: translateY(0); }
}
@keyframes slideUp {
  from { opacity: 0; transform: translateY(25px); }
  to { opacity: 1; transform: translateY(0); }
}

/* ===== MOBILE OPTIMIZATION ===== */
@media (max-width: 768px) {
  .categories-grid {
    grid-template-columns: 1fr;
    gap: 30px;
  }
  .category-block {
    padding: 18px 16px 22px;
  }

  .products-row {
    gap: 12px;
    padding: 8px 4px 12px;
    overflow-x: auto !important;
    -webkit-overflow-scrolling: touch;
    scroll-snap-type: x proximity;
    touch-action: pan-x !important;
    cursor: grab;
  }
  .products-row:active { cursor: grabbing; }

  .product-card {
    flex: 0 0 120px;
  }
  .product-card img { height: 100px; }
  .product-card figcaption {
    font-size: 0.85rem;
    padding: 8px 4px;
  }
  .nav-buttons { display: none; }
}

/* ===== DESKTOP ===== */
@media (min-width: 992px) {
  .categories-grid { gap: 50px; }
  .product-card img { height: 150px; }
}
